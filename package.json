{
  "name": "computerapp",
  "version": "1.0.0",
  "description": "end to end tests for computerapp app",
  "main": "wdio.config.js",
  "scripts": {
    "local-webserver": "npm start --prefix=../",
    "test": "rm -rf reports; node node_modules/webdriverio/bin/wdio; wdio_ret=$?; forever stopall",
